            ],
            "childTitle": {
              "markup": "<p>Find answers on our website</p>",
              "blocks": {
                "contentData": [],
                "settingsData": []
              }
            },
            "childSubText": {
              "markup": "<p> </p>\n<p style=\"text-align: center;\"> </p>\n<p style=\"text-align: center;\"> </p>\n<p style=\"text-align: center;\">Find all the tools you need to answer your pre-application queries from our Working with us Hub. It's your go-to resource centre full of information<span> to help you.</span></p>",
              "blocks": {
                "contentData": [],
                "settingsData": []
              }
            },
            "childIcon": "umb://media/7cafbb6072d745cab74fbd34cdec5f4f"
          },
          {
            "contentTypeKey": "fd96d2a9-a5c2-42e3-a986-147f8da9f730",
            "udi": "umb://element/9f66617a3b93458bb9cfee431d7d8837",
            "cTALink": [
              {
                "name": "Registration",
                "udi": "umb://document/0e4794f3a62d4bc6ae3a978c29c16763"
              }
            ],
            "childTitle": {
              "markup": "<p>Register for our online application system</p>",
              "blocks": {
                "contentData": [],
                "settingsData": []
              }
            },
            "childSubText": {
              "markup": "<p> </p>\n<p style=\"text-align: center;\"> </p>\n<p style=\"text-align: center;\">Check if you're eligible to register for our online application system and submit business from our<span> </span>Registration page.</p>\n<p style=\"text-align: center;\"> </p>",
              "blocks": {
                "contentData": [],
                "settingsData": []
              }
            },
            "childIcon": "umb://media/2badbfaeb6654c89b0d0dd1d5ab4cae1"
