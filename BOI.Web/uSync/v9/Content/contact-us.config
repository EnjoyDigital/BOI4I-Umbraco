            "childIcon": "umb://media/5e92bcde0a4f4d85bcb5997ae8c13c26",
            "childSubText": {
              "markup": "<p> </p>\n<p style=\"text-align: center;\"> </p>\n<p style=\"text-align: center;\">Start a Live Chat with our <strong>applications in progress query team</strong> and they'll try to get you answers as soon as possible.</p>\n<p style=\"text-align: center;\"> </p>",
