            ],
            "childTitle": {
              "markup": "<p>Send us a case message</p>",
              "blocks": {
                "contentData": [],
                "settingsData": []
              }
            },
            "childSubText": {
              "markup": "<p> </p>\n<p style=\"text-align: center;\"> </p>\n<p style=\"text-align: center;\"> </p>\n<p style=\"text-align: center;\">Login<span> to our online application system and send a message directly to the team looking after your case for an update on it's progress.</span></p>",
              "blocks": {
                "contentData": [],
                "settingsData": []
              }
            },
            "childIcon": "umb://media/01b386bcbfe344f1a8b2426b8e76bc07"
          },
          {
            "contentTypeKey": "fd96d2a9-a5c2-42e3-a986-147f8da9f730",
