@inherits UmbracoViewPage<Contact>

@{
	Layout = "/Views/Shared/_Layout.cshtml";
}

<partial name="~/Views/Shared/Breadcrumb.cshtml" />
<partial name="~/Views/Shared/Hero.cshtml" />

@if (Model.Content != null)
{
	@Html.GetBlockListHtml(Model, "content")
}

@if (Model.Pods != null)
{
	<partial name="~/Views/Partials/Pods.cshtml" />
}

@section Scripts {
	<script type="text/javascript" src="/assets/dist/js/contact.bundle.js" asp-append-version="true"></script>
}